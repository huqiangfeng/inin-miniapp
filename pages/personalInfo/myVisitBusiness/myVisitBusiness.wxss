/* pages/personalInfo/myVisitBusiness/myVisitBusiness.wxss */
.visit{
    width: 100%;
    min-height: 100%;
}
.visit .header{
    width:100%;
    height:80rpx;
    position: relative;
    background:rgba(245,246,248,1);
}
.visit .header .van-dropdown-menu__item{
    justify-content: flex-start;
}
.visit .array{
    width: 100%;
    min-height:240rpx ;
    
}
.visit .array .list{
    width: 690rpx;
    height: 240rpx;
    border-bottom: 1rpx solid #EAEAEA;
    box-sizing: border-box;
    margin: 0 auto;
    -moz-box-sizing: border-box;/* Firefox */
    -webkit-box-sizing: border-box;/* Safari */
}
.visit .array .list .top{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
}
.visit .array .list .top .title{
    font-size:30rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:bold;
    color:rgba(51,51,51,1);
}
.visit .array .list .top .time{
    font-size:22rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    color:rgba(153,153,153,1);
}
.visit .array .list .content{
    font-size:28rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:bold;
    color:rgba(51,51,51,1);
}
.visit .array .list .footer{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
}
.visit .array .list .footer .company_name{
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(102,102,102,1);
}
.visit .array .list .footer .link_status{
    font-size:24rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    
}
.visit .array .list .footer .link_status_success{
    color:#5F7AFF;
}
.visit .array .list .footer .link_status_fail{
    color: #999999;
}